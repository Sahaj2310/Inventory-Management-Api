{
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DEVELOPMENT_CONNECTION_STRING"
  },
  "Jwt": {
    "Key": "YOUR_DEVELOPMENT_JWT_SECRET_KEY",
    "Issuer": "YOUR_DEVELOPMENT_ISSUER_URL",
    "Audience": "YOUR_DEVELOPMENT_AUDIENCE_URL",
    "ExpiresInMinutes": 60
  },
  "LowStockSettings": {
    "DefaultThreshold": 10,
    "EnableNotifications": true,
    "NotificationThreshold": 5
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
